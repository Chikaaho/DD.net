<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper  namespace="net.dd.mapper.DdDataMapper">
    <resultMap id="DdDataMapper" type="net.dd.pojo.DdData">
        <id column="file_id" jdbcType="BIGINT" property="fileId"/>
        <result column="file_type" jdbcType="TINYINT" property="fileType"/>
        <result column="file_key" jdbcType="BIGINT" property="fileKey"/>
        <result column="create_time" jdbcType="TIMESTAMP" property="createTime"/>
        <result column="update_time" jdbcType="TIMESTAMP" property="updateTime"/>
    </resultMap>

    <select id="selectByFileId" resultType="net.dd.pojo.DdData">
        SELECT
        file_key
        FROM
        dd_net.dd_data
        where file_id = #(file_id);
    </select>
    <insert id="insertFile">
        INSERT INTO
        dd_net.dd_data
        (file_type, file_key, create_time, update_time)
        VALUES
        (#(file_type), #(file_key), #(create_time), #(update_time))
    </insert>
    <update id="updateFile">

    </update>
    <delete id="deleteById">

    </delete>
</mapper>